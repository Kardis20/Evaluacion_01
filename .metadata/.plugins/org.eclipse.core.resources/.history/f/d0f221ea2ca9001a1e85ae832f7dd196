package Ejercicio_1;


public class Perro extends Animal {
	String sRaza = "Maltes";
	
	public String getsRaza() {
		return sRaza;
	}

	public void setsRaza(String sRaza) {
		this.sRaza = sRaza;
	}

	public Perro() {
		// TODO Auto-generated constructor stub
	}

	public Perro(String sRaza) {
		super();
		this.sRaza = sRaza;
	}

	public Perro(String sNombre, int iPatas) {
		super(sNombre, iPatas);
		// TODO Auto-generated constructor stub
	}
	
	public void ObtenerRaza(){
		
		System.out.println("La raza del perro es "+ this.sRaza);
	}
}
